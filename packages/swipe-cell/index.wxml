<view>
  <wxs src="./index.wxs" module="touch" />
  <view
    wx:if="{{ canIUseWxsEvent }}"
    class="van-swipe-cell canIUseWxsEvent custom-class"
    disable="{{ disable }}"
    catchtap="onClick"
    bindtouchstart="{{ touch.startDrag }}"
    catchtouchmove="{{ catchMove ? 'noop' : '' }}"
    capture-bind:touchmove="{{ touch.onDrag }}"
    bindtouchend="{{ touch.endDrag }}"
    bindtouchcancel="{{ touch.endDrag }}"
  >
    <view style="{{ wrapperStyle }}">
      <view wx:if="{{ leftWidth }}" class="van-swipe-cell__left" data-key="left" catch:tap="onClick">
        <slot name="left" />
      </view>
      <slot />
      <view wx:if="{{ rightWidth }}" class="van-swipe-cell__right" data-key="right" catch:tap="onClick">
        <slot name="right" />
      </view>
    </view>
  </view>
  <view
    wx:else
    class="van-swipe-cell cannot custom-class"
    data-key="cell"
    catchtap="onClick"
    bindtouchstart="startDrag"
    catchtouchmove="{{ catchMove ? 'noop' : '' }}"
    capture-bind:touchmove="onDrag"
    bindtouchend="endDrag"
    bindtouchcancel="endDrag"
  >
    <view style="{{ wrapperStyle }}">
      <view wx:if="{{ leftWidth }}" class="van-swipe-cell__left" data-key="left" catch:tap="onClick">
        <slot name="left" />
      </view>
      <slot />
      <view wx:if="{{ rightWidth }}" class="van-swipe-cell__right" data-key="right" catch:tap="onClick">
        <slot name="right" />
      </view>
    </view>
  </view>
</view>
